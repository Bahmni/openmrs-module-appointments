<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.openmrs.module.appointments.model">
    <class name="AppointmentServiceAttributeType" table="appointment_service_attribute_type" batch-size="50">
        <id name="appointmentServiceAttributeTypeId" type="java.lang.Integer" column="appointment_service_attribute_type_id">
            <generator class="native">
                <param name="sequence">appointment_service_attribute_type_id_seq</param>
            </generator>
        </id>

        <property name="name" type="java.lang.String" column="name" not-null="true" length="255" unique="true"/>
        <property name="description" type="java.lang.String" column="description" length="1024"/>

        <property name="datatypeClassname" type="java.lang.String" column="datatype" length="255"/>
        <property name="datatypeConfig" type="java.lang.String" column="datatype_config"/>
        <property name="preferredHandlerClassname" type="java.lang.String" column="preferred_handler" length="255"/>
        <property name="handlerConfig" type="java.lang.String" column="handler_config"/>

        <property name="minOccurs" type="java.lang.Integer" column="min_occurs" not-null="true"/>
        <property name="maxOccurs" type="java.lang.Integer" column="max_occurs"/>

        <property name="retired" type="java.lang.Boolean" column="retired" not-null="true"/>
        <property name="retireReason" type="java.lang.String" column="retire_reason" length="255"/>
        <property name="dateRetired" type="java.util.Date" column="date_retired" length="19"/>
        <many-to-one name="retiredBy" class="org.openmrs.User" column="retired_by"/>

        <property name="dateCreated" type="java.util.Date" column="date_created" not-null="true" length="19"/>
        <property name="dateChanged" type="java.util.Date" column="date_changed" length="19"/>
        <property name="uuid" type="java.lang.String" column="uuid" length="38" unique="true" not-null="true"/>

        <many-to-one name="creator" class="org.openmrs.User" column="creator" not-null="true"/>
        <many-to-one name="changedBy" class="org.openmrs.User" column="changed_by"/>
    </class>
</hibernate-mapping>
